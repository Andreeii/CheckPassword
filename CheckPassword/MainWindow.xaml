<Window x:Class="CheckPassword.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CheckPassword"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Send" Content="Send" Margin="42,296,517,72" Click="Button_Click" FontSize="20"/>
        <Button x:Name="Check" Content="Check" Margin="385,296,168,72" Click="Button_Click_1" FontSize="20"/>
        <TextBox x:Name="textPassword" TextWrapping="Wrap" Margin="385,190,111,187" TextChanged="TextBox_TextChanged" FontSize="20"/>
        <TextBox x:Name="textEmail" TextWrapping="Wrap" Margin="42,190,463,187" TextChanged="TextBox_TextChanged_1" FontSize="20"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="385,162,0,0" VerticalAlignment="Top" Height="28" Width="96"/>
        <Label Content="Email" HorizontalAlignment="Left" Margin="42,159,0,0" VerticalAlignment="Top" Width="81"/>

    </Grid>
</Window>
